{
  "name": "vue-paper-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prod": "vue-cli-service build --dest ../html",
    "build": "vue-cli-service build",
    "build-hshv-prod": "vue-cli-service build --mode hasonhaivan-prod",
    "build-hv-prod": "vue-cli-service build --mode haivan-prod",
    "build-vt-prod": "vue-cli-service build --mode vungtau-prod",
    "build-sv-prod": "vue-cli-service build --mode saoviet-prod",
    "build-hshv-dev": "vue-cli-service build --mode hasonhaivan-dev",
    "build-hv-dev": "vue-cli-service build --mode haivan-dev",
    "build-vt-dev": "vue-cli-service build --mode vungtau-dev",
    "e2e": "node test/e2e/runner.js",
    "lint": "vue-cli-service lint",
    "lint-fix": "vue-cli-service lint --fix",
    "dev": "vue-cli-service serve --open",
    "dev-hshv": "vue-cli-service serve --mode hasonhaivan-dev --open",
    "dev-hv": "vue-cli-service serve --mode haivan-dev --open",
    "dev-vt": "vue-cli-service serve --mode vungtau-dev --open"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.3.1",
    "chartist": "^0.11.0",
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "element-ui": "^2.0.3",
    "es6-promise": "^4.2.4",
    "jquery": "^3.4.1",
    "jwt-decode": "^2.2.0",
    "leaflet": "^1.5.1",
    "moment": "^2.24.0",
    "perfect-scrollbar": "0.7.1",
    "popper.js": "^1.15.0",
    "redis": "^2.8.0",
    "sockjs-client": "^1.1.4",
    "sweetalert2": "^6.11.5",
    "vue": "^2.5.13",
    "vue-clickaway": "^2.1.0",
    "vue-cookies": "^1.5.13",
    "vue-jquery": "^1.0.6",
    "vue-mqtt": "^2.0.3",
    "vue-nav-tabs": "^0.5.4",
    "vue-notifyjs": "^0.3.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.6.10",
    "vue2-leaflet": "^2.2.1",
    "vuetify": "^1.5.16",
    "vuex": "^3.1.1",
    "webstomp-client": "^1.0.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.9",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.9",
    "@vue/cli-service": "^3.0.0-beta.9",
    "@vue/eslint-config-prettier": "^3.0.0-beta.9",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7"
  },
  "description": "A sample admin dashboard based on paper dashboard UI template",
  "author": "cristian.jora <joracristi@gmail.com>",
  "engines": {
    "node": ">= 8.1.4",
    "npm": ">= 5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
